    "version": "0.2.0",
    "configurations": [
        {
            "name": "Sync DB",
            "request": "launch",
            "runtimeArgs": ["run", "syncdb"],
            "runtimeExecutable": "npm",
            "skipFiles": ["<node_internals>/**"],
            "type": "node"
        },
        {
            "name": "Reset DB Start & Deploy",
            "request": "launch",
            "runtimeArgs": ["syncdb", "deploy", "start"],
            "runtimeExecutable": "npm",
            "skipFiles": ["<node_internals>/**"],
            "type": "node"
        },
        {
            "name": "Start Deploy",
            "request": "launch",
            "runtimeArgs": ["run", "start"],
            "runtimeExecutable": "npm",
            "skipFiles": ["<node_internals>/**"],
            "type": "node"
        },
        {
            "name": "Dev Deploy",
            "request": "launch",
            "runtimeArgs": ["run", "dev"],
            "runtimeExecutable": "npm",
            "skipFiles": ["<node_internals>/**"],
            "type": "node"
        }
    ]
}
